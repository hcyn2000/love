.box{
    line-height: 50rpx;
    font-size: 50rpx;
    width: 100%;
    margin:20rpx 0;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.title{
    position: absolute;
    top:10rpx;
    right: 10rpx;
}